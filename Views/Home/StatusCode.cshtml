@model int
@{
    ViewData["Title"] = "Oops";
}

@if (Model == 404) 
{
    <h1 class="text-danger">404 - Page Not Found</h1>
    <p class="text-muted"> Page you requested doesn't exist</p>
}

else if (Model == 403) 
{
    <h1 class="text-danger">403 - Forbidden</h1>
    <p class="text-muted"><Thead> You don't have permission to access this page.</Thead></p>
}

else 
{
    <h1 class="text-danger">Error @Model</h1>
    <p class="text-muted"> Something went wrong. Please try again later.</p>
}
